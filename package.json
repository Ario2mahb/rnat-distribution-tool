{
  "name": "rnat-distribution",
  "version": "1.0.0",
  "description": "RNat distribution tool",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/ && yarn tsc -b",
    "distribute": "yarn ts-node src/main.ts",
    "typechain-web3-v1": "yarn typechain --target=web3-v1 --out-dir typechain-web3-v1 \"abi/+([a-zA-Z0-9_]).json\" 1> nul",
    "artifacts": "npm-run-all typechain-web3-v1"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "web3": "^4",
    "dotenv": "^8.2.0",
    "csv-parse": "^4.15.3"
  },
  "devDependencies": {
    "@typechain/web3-v1": "3.1.0",
    "@types/chai": "4.3.3",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.11.15",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "3.3.0",
    "typechain": "5.1.2",
    "typescript": "^4.1.5"
  }
}
